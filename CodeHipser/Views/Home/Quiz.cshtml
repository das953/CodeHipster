@model CodeHipser.Models.Question
@{
    ViewData["Title"] = "Quiz";
    
}



    <div>@Model.QuestionText</div>
    <div>
        @foreach (var answer in Model.Answers)
        {

            
            <input type="radio" name="answer"/><span>@answer.AnswerText</span>


        }
    </div>
    
    <div>@ViewBag.Greeting</div>





